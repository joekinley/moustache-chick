    // level generator
    public static function generateLevel( width:int, height:int ):Array {

      var i:int = 0, j:int = 0, k:int = 0, myRand:int = 0;

      // initialize empty level
      var level:Array = new Array( width * height );
      for ( i = 0; i < width * height; i++ ) level[ i ] = 0;

      // make borders
      for ( i = 0; i < width * height; i++ ) {
        // basic level borders
        if ( i < width ) level[ i ] = Globals.TILES_WALL; // top border
        if ( i % width == 0 ) level[ i ] = Globals.TILES_WALL; // left border
        if ( (i+1) % width == 0 ) level[ i ] = Globals.TILES_WALL; // right border
        if ( i > width * height - width ) level[ i ] = Globals.TILES_WALL; // bottom border
      }

      // add platforms to jump on
      for ( i = 0; i < width * height; i++ ) {

        if ( level[ i ] == 0 && Math.random( ) * 100 < Globals.WORLDGEN_FLOOR_RANDOM ) {
          level[ i ] = 21;
        }
      }

      // grow platforms
      for ( j = 0; j < Globals.WORLDGEN_EVOLUTION_RUNS; j++ ) {
        for ( i = 0; i < width * height; i++ ) {
          if ( level[ i ] == 21 ) {
            myRand = Math.random( ) * 100;

            if ( myRand < Globals.WORLDGEN_GROW_RANDOM ) {
              if ( myRand % 2 == 0 ) {
                level[ i - 1 ] = 21;
              } else {
                level[ i + 1] = 21;
              }
            }
          }
        }
      }

      // check for at least 5x3 destination between two tiles
      var leftmost:int, rightmost:int, bottommost:int, helper:int, needsFloor:Boolean;
      for ( i = width*4; i < width * height; i++ ) {

        // only floors shall check
        if ( level[ i ] == 21 ) {

          // reset helper variables
          leftmost = i; rightmost = i; bottommost = i;
          needsFloor = true;

          // calculate borders
          // leftmost
          helper = i;
          while ( helper-- % width != 0 && i-helper < 2 ) leftmost--;
          // rightmost
          helper = i;
          while ( ++helper % width != 0 && i - helper < 2 ) rightmost++;
          // bottommost
          helper = 0;
          while ( i + helper++ * width < width * height && helper < 3 ) bottommost++;

          // columns
          for ( j = (leftmost-i); j < rightmost; j++ ) { // max 7 left to right
            // rows
            for ( k = 0; k < bottommost; k++ ) {
              if ( level[ j + k * width ] == 21 ) needsFloor = false;
            }
          }

          // if needs floor, create one either left or right bottom of myself
          if ( needsFloor ) {
            if ( Math.random( ) * 100 % 2 == 0 ) { // left
              level[ i - leftmost + ( bottommost * width ) ] = 21;
            } else { // right
              level[ i + rightmost + ( bottommost * width ) ] = 21;
            }
          }
        }
      }

      // no two platforms ontop of each other
      var bottom:int = 0;
      for ( i = width; i < width * height; i++ ) {

        if( level[ i ] == 21 ) {
          bottom = i + width;
          if ( level[ bottom ] == 21 ) {
            if ( Math.random( ) * 100 % 2 == 0 ) { // get rid of myself
              level[ i ] = 0;
            } else { // get rid of bottom
              level[ bottom ] = 0;
            }
          }
        }
      }

      // add random collectibles and hearts
      for ( i = 0; i < width * height; i++ ) {
        if ( level[ i ] == 0 ) {
          if ( Math.random( ) * 1000 < Globals.WORLDGEN_COIN_RANDOM ) {
            level[ i ] = Globals.TILES_COLLECTIBLE_INDICATOR;
          }
          if ( Math.random( ) * 1000 < Globals.WORLDGEN_HEART_RANDOM ) {
            level[ i ] = Globals.TILES_HEART_INDICATOR;
          }
        }
      }

      // set player position at leftmost and bottommost 0 tile
      var playerSet:Boolean = false;
      do {
        for ( j = height; j > 0; j-- ) {
          for ( i = 0; i < width; i++ ) {
            if ( !playerSet && level[ width * j - width + i ] == 0 ) {
              level[ width * j - width + i ] = Globals.TILES_PLAYER_START;
              playerSet = true;
            }
          }
        }
      } while ( !playerSet );

      // place exit
      var exitSet:Boolean = false;
      do {
        for ( j = 0; j < height; j++ ) {
          for ( i = 0; i < width; i++ ) {
            if ( !exitSet && level[ width * j - width + i ] == 0 ) {
              level[ width * j - width + i ] = Globals.TILES_EXIT;
              exitSet = true;
            }
          }
        }
      } while ( !exitSet );

      // set lava sources
      var hasOne:Boolean = false;
      do {
        for ( i = 0; i < width; i++ ) {
          if ( Math.random( ) * 100 < Globals.WORLDGEN_LAVASOURCE_RANDOM && level[ i + width ] == 0 ) {
            level[ i ] = Globals.TILES_LAVA_SOURCE;
            hasOne = true;
          }
        }
      } while ( !hasOne ); // at least one lavasource is needed

      // return generated level
      return level;
    }

